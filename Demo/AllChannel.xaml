<Window x:Class="Demo.AllChannel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AllChanel"  Height="786" Width="1024" WindowState="Maximized">
    <Window.Background>
        <ImageBrush ImageSource="Images\Background.jpg"></ImageBrush>
    </Window.Background>
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="20">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="18*" />
                <RowDefinition Height="50*" />
                <RowDefinition Height="33*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="20*"/>
                <ColumnDefinition Width="60*"/>
                <ColumnDefinition Width="20*"/>
            </Grid.ColumnDefinitions>
            <Rectangle Cursor="Hand" Grid.Column="1" Fill="#FF89EEA9" HorizontalAlignment="Left" Height="156" Margin="113,187,0,0" Grid.Row="1" Stroke="White" StrokeThickness="5"  VerticalAlignment="Top" Width="241"  RadiusY="13.5" RadiusX="13.5" MouseDown="AllChannel_MouseDown" />
            <Label Cursor="Hand" Content="All Channels" Grid.Column="1" HorizontalAlignment="Left" Margin="140,235,0,0" Grid.Row="1" VerticalAlignment="Top" Width="186" Height="44" FontSize="30" Foreground="Red" FontFamily="Segoe UI" MouseDown="AllChannel_MouseDown" />
        </Grid>
    </StackPanel>
</Window>
